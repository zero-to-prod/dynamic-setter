{
  "type": "library",
  "name": "zero-to-prod/dynamic-setter",
  "description": "Fluently set class properties with dynamic methods.",
  "keywords": [
    "dynamic-setter",
    "Dynamic Setter",
    "zero-to-prod"
  ],
  "homepage": "https://github.com/zero-to-prod/dynamic-setter",
  "license": "MIT",
  "readme": "./README.md",
  "authors": [
    {
      "name": "David Smith",
      "email": "dave0016@gmail.com",
      "homepage": "https://davidsmith.blog/",
      "role": "maintainer"
    }
  ],
  "funding": [
    {
      "type": "GitHub",
      "url": "https://github.com/sponsors/zero-to-prod"
    }
  ],
  "scripts": {
    "test-all": [
      "sh test.sh"
    ],
    "docker": [
      "docker compose run --rm "
    ]
  },
  "scripts-descriptions": {
    "test-all": "tests all php versions",
    "docker": "runs a docker container"
  },
  "require": {
    "php": "^7.1"
  },
  "autoload": {
    "psr-4": {
      "Zerotoprod\\DynamicSetter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  }
}
